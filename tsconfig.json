    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "types/*.ts"
  ],
  "include": [
    "**/*",
  ]
}